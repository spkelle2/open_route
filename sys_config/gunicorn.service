[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/open_route/open_route
ExecStart=/home/ubuntu/miniconda3/envs/env_or/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/open_route/open_route/open_route.sock open_route.wsgi:application

[Install]
WantedBy=multi-user.target
